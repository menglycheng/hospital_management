<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
         <record id="patient_seq" model="ir.sequence">
            <field name="name">Patient sequence</field>
            <field name="code">patient.data</field>
            <field name="prefix">PR</field>
            <field name="padding">8</field>
            <field eval="False" name="company_id"/>
        </record>
        <record id="appointment_seq" model="ir.sequence">
            <field name="name">Appointment sequence</field>
            <field name="code">patient.appointment</field>
            <field name="prefix">APR</field>
            <field name="padding">8</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>